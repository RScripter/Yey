-- Load the library
local OrionLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/thanhdat4461/OrionMoblie/main/source"))()

-- Create the main window
local Window = OrionLib:MakeWindow({
    Name = "Made By Player And Vengeance",
    HidePremium = false,
    SaveConfig = true,
    ConfigFolder = "OrionTest"
})

-- Tab 1: Player tab
local PlayerTab = Window:MakeTab({
    Name = "Player",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

-- Movement section in Player tab
local MovementSection = PlayerTab:AddSection({
    Name = "Movement"
})

-- Walkspeed slider
PlayerTab:AddSlider({
    Name = "Walkspeed",
    Min = 16,
    Max = 500,
    Default = 16,
    Color = Color3.fromRGB(255, 255, 255),
    Increment = 1,
    ValueName = "WS",
    Callback = function(Value)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
    end
})

-- Jump height slider
PlayerTab:AddSlider({
    Name = "Jump Height",
    Min = 16,
    Max = 500,
    Default = 5,
    Color = Color3.fromRGB(255, 255, 255),
    Increment = 1,
    ValueName = "Height",
    Callback = function(Value)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
    end
})

-- Tab 2: mm2 tab
local ToolsTab = Window:MakeTab({
    Name = "mm2",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

-- Tools section in Tools tab
local ToolsSection = ToolsTab:AddSection({
    Name = "mm2"
})

-- yahrm button
ToolsTab:AddButton({
    Name = "yahrm",
    Callback = function()
        -- Replace with your tool script URL
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Joystickplays/psychic-octo-invention/main/yarhm.lua", false))()
    end
})

-- symphony hub button
ToolsTab:AddButton({
    Name = "symphony hub",
    Callback = function()
        -- Replace with your tool script URL
        loadstring(game:HttpGet('https://raw.githubusercontent.com/ThatSick/ArrayField/main/SymphonyHub.lua'))()
    end
})

